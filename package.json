{
  "name": "storybook-docs-bug-repro",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
	"docs:json": "compodoc -p ./tsconfig.json -e json -d .",
	"storybook": "npm run docs:json && start-storybook -p 6006",
	"storybook:nodocs": "echo '{}' > documentation.json && STORYBOOK_CI=true start-storybook -p 6006",
	"build-storybook": "npm run docs:json && NODE_OPTIONS=--max_old_space_size=2048 build-storybook",
	"build-storybook:nodocs": "NODE_OPTIONS=--max_old_space_size=2048 build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.4",
